{
	"name": "sample",
	"version": "0.0.1",
	"description": "Sample Api ",
	"author": "sample",
	"main": "index.js",
	"type":"module",
	"private": false,
	"engines": {
		"node": ">=16.14.2",
		"npm": ">=8.5.2",
		"yarn": ">=1.22.17"
	},
	"scripts": {
		"start": "gulp serve",
		"start:debug": "cross-env DEBUG=cyou-rest-api:* yarn start",
		"build": "gulp",
		"copy": "gulp copy",
		"babel": "gulp babel",
		"lint": "esw *.js src config --color",
		"lint-fix": "esw *.js src config --color --fix",
		"lint:watch": "yarn lint -- --watch",
		"fix-js": "eslint-auto-fix \"src/**/.js\" \"config/**/.js\" \"config/*.js\"",
		"beautify": "es-beautifier -u -c .eslintrc.js *.js src",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _jest -- --ui bdd --reporter spec --colors --compilers js:babel-core/register server/tests --recursive",
		"test:check-coverage": "yarn test:coverage && istanbul check-coverage",
		"report-coverage": "coveralls < ./coverage/lcov.info"
	},
	"keywords": [
		"express",
		"node",
		"node.js",
		"mongodb",
		"mongoose",
		"es6",
		"jest",
		"istanbul",
		"REST",
		"API",
		"boilerplate"
	],
	"jest": {
		"automock": false,
		"bail": false,
		"verbose": true,
		"setupFiles": [
			"./jest-setup.js"
		],
		"testMatch": [
			"**/Tests/**/*.js",
			"**/?(*.)(spec|test|integration).js?(x)"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/coverage/",
			"/dist/"
		],
		"testEnvironment": "node"
	},
